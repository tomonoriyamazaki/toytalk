{
  "expo": {
    "name": "ToyTalk",
    "slug": "toytalk",
    "version": "0.5.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "toytalk",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.zakicorp.toytalk",
      "buildNumber": "13",
      "supportsTablet": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "音声認識のためにマイクを使用します。",
        "NSSpeechRecognitionUsageDescription": "音声を文字に変換するために音声認識を使用します。",
        "NSPhotoLibraryUsageDescription": "このアプリはユーザーの体験向上のために写真ライブラリへアクセスする可能性があります。",
        "NSPhotoLibraryAddUsageDescription": "このアプリは生成した画像やコンテンツを保存するために写真ライブラリを使用する可能性があります。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zakicorp.toytalk"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98de720f-f917-4ca2-93db-81ea4dd4c9a9"
      }
    },
    "owner": "zackey",
    "plugins": [
      "expo-audio"
    ]
  }
}
