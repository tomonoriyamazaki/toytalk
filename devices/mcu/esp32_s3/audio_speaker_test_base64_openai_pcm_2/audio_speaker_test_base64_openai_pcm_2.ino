#include <driver/i2s.h>
#include "mbedtls/base64.h"
#include <string.h>

#define PIN_BCLK   4
#define PIN_LRC    3
#define PIN_DOUT   5
#define PIN_AMP_SD 6

// ‚òÖ„Åì„Åì„Å´ base64 „ÅÆPCM„Éá„Éº„Çø„ÇíË≤º„Çã
//   ‰æã: "AAAA////4AEBA..." „Åø„Åü„ÅÑ„Å™1Êú¨„ÅÆÈï∑„ÅÑÊñáÂ≠óÂàó
const char* BASE64_DATA =
  "";

// „É¢„Éé„É©„É´PCM ‚Üí „Çπ„ÉÜ„É¨„Ç™PCM Â§âÊèõ
void monoToStereo(int16_t* mono, int16_t* stereo, size_t samples) {
  for (size_t i = 0; i < samples; i++) {
    stereo[2 * i]     = mono[i]; // Left
    stereo[2 * i + 1] = mono[i]; // Right
  }
}

void setup() {
  Serial.begin(115200);
  Serial.println("üîä I2S Stereo Playback Test (base64 decode)");

  pinMode(PIN_AMP_SD, OUTPUT);
  digitalWrite(PIN_AMP_SD, HIGH);

  // ===== I2S Ë®≠ÂÆö =====
  i2s_config_t cfg = {
      .mode = (i2s_mode_t)(I2S_MODE_MASTER | I2S_MODE_TX),
      .sample_rate = 24000,
      .bits_per_sample = I2S_BITS_PER_SAMPLE_16BIT,
      .channel_format = I2S_CHANNEL_FMT_RIGHT_LEFT,
      .communication_format = (i2s_comm_format_t)(I2S_COMM_FORMAT_I2S | I2S_COMM_FORMAT_I2S_MSB),
      .intr_alloc_flags = 0,
      .dma_buf_count = 8,
      .dma_buf_len = 1024,
      .use_apll = true,
      .tx_desc_auto_clear = true,
      .fixed_mclk = 0
  };

  i2s_pin_config_t pins = {
      .bck_io_num = PIN_BCLK,
      .ws_io_num = PIN_LRC,
      .data_out_num = PIN_DOUT,
      .data_in_num = I2S_PIN_NO_CHANGE
  };

  i2s_driver_install(I2S_NUM_1, &cfg, 0, NULL);
  i2s_set_pin(I2S_NUM_1, &pins);
  i2s_set_clk(I2S_NUM_1, 24000, I2S_BITS_PER_SAMPLE_16BIT, I2S_CHANNEL_STEREO);

  // ===== base64 ‚Üí PCM „Éá„Ç≥„Éº„Éâ =====
  size_t b64_len = strlen(BASE64_DATA);
  if (b64_len == 0) {
    Serial.println("‚ùå BASE64_DATA is empty");
    return;
  }

  // base64 „ÅÆÊúÄÂ§ß„Éá„Ç≥„Éº„Éâ„Çµ„Ç§„Ç∫„ÅØ„Åä„Åä„Çà„Åù 3/4
  size_t pcm_max_len = (b64_len / 4) * 3;
  uint8_t* pcm_buf = (uint8_t*)malloc(pcm_max_len);
  if (!pcm_buf) {
    Serial.println("‚ùå malloc pcm_buf failed");
    return;
  }

  size_t pcm_len = 0;
  int ret = mbedtls_base64_decode(
      pcm_buf,
      pcm_max_len,
      &pcm_len,
      (const unsigned char*)BASE64_DATA,
      b64_len
  );

  if (ret != 0) {
    Serial.printf("‚ùå base64 decode error = %d\n", ret);
    free(pcm_buf);
    return;
  }

  Serial.printf("‚úÖ base64 decoded: %u bytes\n", (unsigned int)pcm_len);

  // ===== „É¢„Éé„É©„É´PCM ‚Üí „Çπ„ÉÜ„É¨„Ç™PCM =====
  size_t mono_bytes   = pcm_len;
  size_t mono_samples = mono_bytes / 2; // int16_t „Å™„ÅÆ„Åß2„Éê„Ç§„Éà

  int16_t* mono = (int16_t*)pcm_buf;

  size_t stereo_bytes = mono_bytes * 2; // „Çπ„ÉÜ„É¨„Ç™Ôºù2ch
  int16_t* stereo = (int16_t*)malloc(stereo_bytes);
  if (!stereo) {
    Serial.println("‚ùå malloc stereo failed");
    free(pcm_buf);
    return;
  }

  monoToStereo(mono, stereo, mono_samples);

  // ===== I2S „ÅßÂÜçÁîü =====
  size_t written = 0;
  esp_err_t err = i2s_write(
      I2S_NUM_1,
      stereo,
      stereo_bytes,
      &written,
      portMAX_DELAY
  );

  Serial.printf("I2S write result = %d, written = %u bytes\n",
                (int)err, (unsigned int)written);

  free(stereo);
  free(pcm_buf);
}

void loop() {
  // „É´„Éº„Éó„Åß‰Ωï„ÇÇ„Åõ„Åö1Âõû„Å†„ÅëÂÜçÁîü
}
