#include <driver/i2s.h>
#include "mbedtls/base64.h"

#define PIN_BCLK   4
#define PIN_LRC    3
#define PIN_DIN    5
#define PIN_AMP_SD 6
#define SAMPLE_RATE 52000  // OpenAI PCM16 は 24kHz 固定

// ---- テスト用PCM Base64（短めでOK）----
const char* BASE64_DATA =
"";


void setup() {
  Serial.begin(115200);
  Serial.println("🔊 Base64→PCM 再生テスト開始");

  // アンプ起動
  pinMode(PIN_AMP_SD, OUTPUT);
  digitalWrite(PIN_AMP_SD, HIGH);

  // ==== I2S構成 ====
  i2s_config_t cfg = {
    .mode = (i2s_mode_t)(I2S_MODE_MASTER | I2S_MODE_TX),
    .sample_rate = SAMPLE_RATE,
    .bits_per_sample = I2S_BITS_PER_SAMPLE_16BIT,
    .channel_format = I2S_CHANNEL_FMT_ONLY_LEFT,        // ← MONO
    .communication_format = I2S_COMM_FORMAT_STAND_I2S,
    .intr_alloc_flags = 0,
    .dma_buf_count = 12,
    .dma_buf_len = 1024,
    .use_apll = false,                                   // ← APLLで安定出力
    .tx_desc_auto_clear = true,
    .fixed_mclk = 0
  };

  i2s_pin_config_t pins = {
    .bck_io_num = PIN_BCLK,
    .ws_io_num = PIN_LRC,
    .data_out_num = PIN_DIN,
    .data_in_num = I2S_PIN_NO_CHANGE
  };

  // ==== I2S初期化 ====
  i2s_driver_install(I2S_NUM_1, &cfg, 0, NULL);
  i2s_set_pin(I2S_NUM_1, &pins);
  i2s_set_clk(I2S_NUM_1, SAMPLE_RATE, I2S_BITS_PER_SAMPLE_16BIT, I2S_CHANNEL_MONO);

  // ==== Base64 デコード ====
  size_t b64Len = strlen(BASE64_DATA);
  size_t pcmBufLen = b64Len * 3 / 4;
  uint8_t* pcmData = (uint8_t*)malloc(pcmBufLen);
  if (!pcmData) {
    Serial.println("❌ malloc failed");
    return;
  }

  size_t outLen = 0;
  int ret = mbedtls_base64_decode(pcmData, pcmBufLen, &outLen, (const unsigned char*)BASE64_DATA, b64Len);
  if (ret != 0) {
    Serial.println("❌ Base64 decode failed");
    free(pcmData);
    return;
  }
  Serial.printf("✅ Base64 decode success. PCM size: %d bytes\n", (int)outLen);

  if (outLen > 44 &&
      pcmData[0] == 'R' && pcmData[1] == 'I' &&
      pcmData[2] == 'F' && pcmData[3] == 'F') {
    Serial.println("📂 WAV header detected → skip 44 bytes");
    pcmData += 44;
    outLen  -= 44;
  } else {
    Serial.println("📂 No WAV header detected (raw PCM)");
  }

  // ==== 先頭16バイトのダンプ ====
  Serial.print("📦 PCM head bytes: ");
  for (int i = 0; i < 16 && i < outLen; i++) {
    Serial.printf("%02X ", pcmData[i]);
  }
  Serial.println();

  // ==== モノラルPCM → ステレオ化（左右同一出力）====
  size_t sampleCount = outLen / 2;
  size_t stereoLen = sampleCount * 2 * 2; // 16bit * 2ch
  int16_t* stereoData = (int16_t*)malloc(stereoLen);
  if (!stereoData) {
    Serial.println("❌ malloc failed (stereo)");
    free(pcmData);
    return;
  }

  int16_t* src = (int16_t*)pcmData;
  int16_t* dst = stereoData;
  for (size_t i = 0; i < sampleCount; i++) {
    int16_t s = src[i];
    uint8_t low = s & 0xFF;
    uint8_t high = (s >> 8) & 0xFF;
    uint16_t swapped = (high << 8) | low;
    *dst++ = swapped;
    *dst++ = swapped;
  }

  // ==== 再生 ====
  size_t written = 0;
  esp_err_t err = i2s_write(I2S_NUM_1, stereoData, stereoLen, &written, portMAX_DELAY);
  if (err != ESP_OK) {
    Serial.printf("❌ i2s_write failed: %d\n", err);
  } else {
    Serial.printf("🎧 再生完了 (%d bytes written)\n", (int)written);
  }


  // ==== 解放 ====
  free(stereoData);
  free(pcmData);
}

void loop() {

}
